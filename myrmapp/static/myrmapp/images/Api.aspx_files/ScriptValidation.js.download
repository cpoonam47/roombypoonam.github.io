function GetStringLength(str)
{
var count =0;
for (var i=0;i<str.length;i++)
{
var strChar = str.charAt(i); 
if (strChar =="}" || strChar == "{" || strChar == "]" || strChar == "[" || strChar == "|" || strChar == "^" || strChar == "~"  || strChar == "\\" )  
count = count+2;  
else
count = count+1; 
} 
return count;
}

